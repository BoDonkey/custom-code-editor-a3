{
    "extends": "punkave",
    "globals": {
        "$": "readonly",
        "_": "readonly",
        "ace": "readonly",
        "apos": "writable",
        "after": "readonly",
        "before": "readonly"
    },
    "plugins": [
        "jest"
    ],
    "env": {
        "es6": true,
        "jest": true,
        "jest/globals": true
    },
    "rules": {
        "indent": "off",
        "space-before-function-paren": "off",
        "semi": "warn",
        "no-return-assign": "off",
        "no-unused-expressions": "off",
        "padded-blocks": "off",
        "eol-last": "off",
        "no-unused-vars": "off",
        "no-debugger": "warn",
        "jest/valid-expect": [
            "error",
            {
                "maxArgs": 2
            }
        ]
    }
}